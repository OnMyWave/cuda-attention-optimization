ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /data01/jonggeunlee/miniconda3/envs/comp2/compiler_compat -fno-strict-overflow -Wsign-compare -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /data01/jonggeunlee/miniconda3/envs/comp2/include -fPIC -O2 -isystem /data01/jonggeunlee/miniconda3/envs/comp2/include -fPIC -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include/TH -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include/THC -I/usr/local/cuda/include -I/data01/jonggeunlee/miniconda3/envs/comp2/include/python3.12 -c
post_cflags = -O3 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=attention_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include/TH -I/data01/jonggeunlee/miniconda3/envs/comp2/lib/python3.12/site-packages/torch/include/THC -I/usr/local/cuda/include -I/data01/jonggeunlee/miniconda3/envs/comp2/include/python3.12 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 -gencode=arch=compute_80,code=sm_80 --use_fast_math -lineinfo -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=attention_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





build /data01/jonggeunlee/cuda-attention-optimization/phase2_naive/build/temp.linux-x86_64-cpython-312/attention_cuda.o: compile /data01/jonggeunlee/cuda-attention-optimization/phase2_naive/attention_cuda.cpp
build /data01/jonggeunlee/cuda-attention-optimization/phase2_naive/build/temp.linux-x86_64-cpython-312/attention_kernel.o: cuda_compile /data01/jonggeunlee/cuda-attention-optimization/phase2_naive/attention_kernel.cu






